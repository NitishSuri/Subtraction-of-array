# Subtraction-of-array
//23rd program on c
#include<stdio.h>
int main()
{
int a[3][3],b[3][3],diff[3][3],i,j;
for(i=0;i<3;i++)
{
  for(j=0;j<3;j++)
   {
     printf("Enter the elements  \n");
     scanf("%d",&a[i][j]);
    }
}
for(i=0;i<3;i++)
{
  for(j=0;j<3;j++)
   {
     printf("Enter the elements of the second matrix \n");
     scanf("%d",&b[i][j]);
    }
}													
printf("The difference of the matrices is \n");
for(i=0;i<3;i++)
    {
      for(j=0;j<3;j++)
        {
           diff[i][j]=a[i][j]-b[i][j];
           printf("%d \t",diff[i][j]);
          }
     printf("\n");
    }
return(0);
} 

